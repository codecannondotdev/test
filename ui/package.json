{
	"name": "ui",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "run-p type-check \"build-only {@}\" --",
		"preview": "vite preview",
		"test:unit": "vitest",
		"test:e2e": "playwright test",
		"build-only": "vite build",
		"type-check": "vue-tsc -p tsconfig.app.json --noEmit",
		"lint": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
		"lint-check": "ESLINT_USE_FLAT_CONFIG=false eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
		"format": "prettier --write src/",
		"format-check": "prettier --check src/",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run --typecheck",
		"coverage": "vitest run --coverage"
	},
	"overrides": {
		"vite": "npm:rolldown-vite@latest"
	},
	"dependencies": {
		"@primevue/themes": "4.3.9",
		"@sentry/vite-plugin": "^4.3.0",
		"@sentry/vue": "^10.15.0",
		"axios": "^1.12.2",
		"date-fns": "^4.1.0",
		"pinia": "^3.0.3",
		"posthog-js": "^1.268.8",
		"primevue": "4.3.9",
		"sass": "^1.93.2",
		"vue": "^3.5.22",
		"vue-router": "^4.5.1"
	},
	"devDependencies": {
		"@playwright/test": "^1.55.1",
		"@rushstack/eslint-patch": "^1.3.3",
		"@tsconfig/node18": "^18.2.2",
		"@types/jsdom": "^21.1.7",
		"@types/node": "^24.5.2",
		"@vitejs/plugin-vue": "^6.0.1",
		"@vue/eslint-config-prettier": "^8.0.0",
		"@vue/eslint-config-typescript": "^12.0.0",
		"@vue/test-utils": "^2.4.6",
		"@vue/tsconfig": "^0.8.1",
		"eslint": "^8.49.0",
		"eslint-plugin-vue": "^9.17.0",
		"jsdom": "^27.0.0",
		"npm-run-all2": "^8.0.4",
		"prettier": "^3.6.2",
		"rolldown-vite": "latest",
		"typescript": "~5.9.2",
		"vitest": "^3.2.4",
		"vue-tsc": "^3.1.0"
	}
}
